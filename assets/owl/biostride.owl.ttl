            owl:allValuesFrom xsd:float ;
            owl:onProperty biostride_schema:pixel_size_max ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty biostride_schema:detector_dimensions ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty biostride_schema:phase_plate ],
